{
  "presets":[
      "es2015", "react", "stage-0"
  ],
  "env": {
    "development": {
      "presets": ["es2015", "react", "stage-0"],
      "plugins": [
        "transform-runtime",
        "transform-object-rest-spread",
        "transform-export-extensions",
        "transform-class-properties"
      ],
      "presets": ["react-hmre"]
    }
  },
  "env": {
    "build": {
      "presets": ["es2015", "react", "stage-0"],
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    }
  }
}
